        {
          "id": "matrix.instructions",
          "type": "text",
          "role": "paragraph",
          "text": "Select “YES” or “NO” for each of the following statements. For any “NO” answer, provide an explanation in Comments. If sufficient information is not available, select “NAV” and re-submit when available."
        },
        {
          "id": "matrix.table",
          "type": "table",
          "columns": [
            { "key": "nav", "header": "NAV", "width": 40 },
            { "key": "yes", "header": "YES", "width": 40 },
            { "key": "no",  "header": "NO",  "width": 40 },
            { "key": "statement", "header": null, "width": "*" }
          ],
          "rows": [
            { "id": "s1", "statement": "There is no payment default or performance default under the Note or Security Instrument.", "field": { "type": "radio", "name": "stmt1", "options": ["nav","yes","no"] } },
            { "id": "s2", "statement": "Total funds available from insurance proceeds (combined with other funds the Borrower will escrow) are sufficient to fully repair the Property.", "field": { "type": "radio", "name": "stmt2", "options": ["nav","yes","no"] } },
            { "id": "s3", "statement": "The Property, as repaired, should be capable of generating enough income to cover operating expenses, reserve requirements, and debt service.", "field": { "type": "radio", "name": "stmt3", "options": ["nav","yes","no"] } },
            { "id": "s4", "statement": "There are no zoning or land use restrictions that would preclude rebuilding/restoration to pre-casualty condition and characteristics.", "field": { "type": "radio", "name": "stmt4", "options": ["nav","yes","no"] } },
            { "id": "s5", "statement": "Restoration and repair can be completed by the earlier of one year from the date of loss or the Note maturity date.", "field": { "type": "radio", "name": "stmt5", "options": ["nav","yes","no"] } },
            { "id": "s6", "statement": "Servicer has inspected the Property since being notified of the loss. (Attach 834sdf and photos for any “YES”.)", "field": { "type": "radio", "name": "stmt6", "options": ["nav","yes","no"] } },
            { "id": "s7", "statement": "The attached loss draft is the first loss draft in connection with this loss.", "field": { "type": "radio", "name": "stmt7", "options": ["nav","yes","no"] } },
            { "id": "s8", "statement": "There was no loss of life or serious injury requiring hospitalization due to the casualty loss.", "field": { "type": "radio", "name": "stmt8", "options": ["nav","yes","no"] } },
            { "id": "s9", "statement": "Servicer has made a timely recommendation to add 23du as loss payee on the insurance policy and loss draft check.", "field": { "type": "radio", "name": "stmt9", "options": ["nav","yes","no"] } }
